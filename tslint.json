{
  "extends": "tslint-microsoft-contrib",
  "rules": {
    "export-name": false,
    "indent": [true, 2],
    "match-default-export-name": false,
    "max-line-length": [true, 80],
    "no-suspicious-comment": false,
    "no-unsafe-any": false,
    "typedef": [
      true,
      "call-signature",
      "member-variable-declaration",
      "parameter",
      "variable-declaration"
    ]
  }
}
